//Complexidade de Algoritmos

//Exemplo 1 O(1) tempo constante
maretial = caixa[0]

//Exemplo 2 O(Log (N) - Tempo Logaritmico
materiais = ["aluminio", "cobalto", "ferro", "ouro", "prata"]
materiais.sort()

def busca_binaria(lista, alvo):
    inicio = 0
    fim = len(lista) - 1
    while inicio <= fim:
        meio = (inicio + fim) // 2
        if lista[meio] == alvo:
            return meio
        elif lista[meio] < alvo:
            inicio = meio + 1
        else:
            fim = meio - 1
    return -1

//Exemplo 3 Chamado de O(N) - Tempo Linear
def busca_linear(lista, alvo):
    for item in lista:
        if item == "prata":
            break
        if item == alvo:
            return True
    return False

materiais = ["aluminio", "ferro", "cobalto", "prata", "cobre"]

//Exemplo 4 O(N LOG (N)) - tempo quase linear
caixa.sort()

//Exemplo 5 O(N^2) - Tempo QuadrÃ¡tico
for i in range(len(caixa)):
    for j in range(i + 1, len(caixa)):
        if caixa[i] == estante[j]:
            printf("Reperido", caixa[i])

//Exemplo 6 (O^2N) - Tempo exponencial
from intertools import combinations
materiais = ["aluminio", "ferro", "cobalto", "prata", "ouro"]
combinacoes = []
for i in range (len(materiais) + 1):
    for c in combinations(matetiais, i):
for combo in combinacoes:
    print(combo)

//Exemplo 7F O(N!) - Tempo Fatorial
from itertools import permutations
for ordem in permutations(caixa):
    print(ordem)
